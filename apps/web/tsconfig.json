{
	"extends": "@ckt1031/tsconfig/default",
	"exclude": ["node_modules", "components/ui"],
	"include": [
		"app",
		"components",
		"styles",
		"utils",
		"*.ts",
		"next-env.d.ts",
		".next/types/**/*.ts",
		"next.config.js"
	],
	"compilerOptions": {
		"moduleResolution": "Bundler",
		"lib": ["ESNext", "dom"],
		"paths": {
			"@/*": ["./*"]
		},
		"jsx": "preserve",
		"plugins": [
			{
				"name": "next"
			}
		]
	}
}
