{
  "$schema": "http://json.schemastore.org/tsconfig",
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "src"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types"
    ],
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}