{
	"compilerOptions": {
		"resolveJsonModule": true,
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"target": "ESNext",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["**/*.ts", ".nuxt/schema/nuxt.schema.d.ts"],
	"exclude": ["node_modules", "dist", ".nuxt", ".output", ".vercel", "web"]
}
