{
  "name": "cktsun-helper",
  "type": "module",
  "scripts": {
    "build": "tsup src --minify --format esm --sourcemap --clean --target esnext",
    "dev": "cross-env NODE_ENV=development nodemon",
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --fix .",
    "start": "node --enable-source-maps -r dotenv/config dist/index.js | pino-pretty",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@extractus/article-extractor": "7.2.15",
    "@fastify/compress": "6.3.0",
    "@fastify/cookie": "8.3.0",
    "@fastify/rate-limit": "8.0.0",
    "@fastify/session": "10.3.0",
    "@fastify/view": "7.4.1",
    "@fastify/websocket": "8.0.0",
    "@iamtraction/google-translate": "2.0.1",
    "@sentry/node": "7.52.1",
    "axios": "1.4.0",
    "croner": "6.0.3",
    "cross-env": "7.0.3",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.7",
    "discord.js": "14.11.0",
    "dotenv": "16.0.3",
    "ejs": "3.1.9",
    "fastify": "4.17.0",
    "glob": "10.2.5",
    "html-to-text": "9.0.5",
    "mongoose": "7.1.1",
    "node-cache": "5.1.2",
    "nodemon": "2.0.22",
    "normalize-url": "8.0.0",
    "pidusage": "3.0.2",
    "pino": "8.14.1",
    "pino-pretty": "10.0.0",
    "rss-parser": "3.13.0",
    "s-ago": "2.2.0",
    "ws": "8.13.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@ckt1031/eslint-config": "1.1.6",
    "@ckt1031/prettier-config": "1.1.6",
    "@ckt1031/tsconfig": "1.1.6",
    "@types/crypto-js": "4.1.1",
    "@types/ejs": "3.1.2",
    "@types/html-to-text": "9.0.0",
    "@types/pidusage": "2.0.2",
    "@types/ws": "8.5.4",
    "eslint": "8.40.0",
    "prettier": "2.8.8",
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "nodemonConfig": {
    "exec": "pnpm run build && pnpm run start",
    "ext": "ts,mjs,js,cjs,json,graphql",
    "ignore": [
      "dist",
      "src/**/*.test.ts",
      "node_modules"
    ],
    "watch": [
      "src"
    ]
  },
  "packageManager": "pnpm@8.5.1",
  "prettier": "@ckt1031/prettier-config"
}
