{
	"name": "news-bot",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "wrangler dev src/index.ts",
		"vercel-build": "pnpm run generate-tool-api-schema && next build && pnpm run register-commands",
		"deploy": "wrangler deploy --minify",
		"lint": "tsc && biome check .",
		"lint:fix": "tsc && biome format . --write && biome check . --write",
		"generate": "drizzle-kit generate",
		"migrate": "drizzle-kit migrate",
		"typecheck": "tsc",
		"test": "vitest run tests/unit",
		"register-commands": "tsx scripts/register-commands.ts",
		"generate-tool-api-schema": "tsx scripts/generate-tool-api-schema.ts"
	},
	"dependencies": {
		"@hono/sentry": "^1.1.0",
		"@hono/zod-validator": "^0.2.2",
		"@iamtraction/google-translate": "^2.0.1",
		"@langchain/core": "^0.2.6",
		"@langchain/openai": "^0.1.2",
		"@vercel/functions": "^1.0.2",
		"consola": "^3.2.3",
		"discord-api-types": "^0.37.87",
		"discord-interactions": "^3.4.0",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.31.2",
		"drizzle-zod": "^0.5.1",
		"fast-xml-parser": "^4.4.0",
		"hono": "^4.4.4",
		"html-entities": "^2.5.2",
		"js-tiktoken": "^1.0.12",
		"langchain": "^0.2.5",
		"langfuse-langchain": "^3.11.2",
		"mustache": "^4.2.0",
		"nanoid": "^5.0.7",
		"next": "^14.2.3",
		"ofetch": "^1.3.4",
		"openapi-fetch": "^0.9.7",
		"postgres": "^3.4.4",
		"toucan-js": "^3.4.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.0",
		"@cloudflare/workers-types": "^4.20240605.0",
		"@types/mustache": "^4.2.5",
		"@types/node": "^20.14.2",
		"@types/react": "^18.3.3",
		"drizzle-kit": "^0.22.6",
		"openapi-typescript": "^6.7.6",
		"supabase": "^1.172.2",
		"tsx": "^4.15.0",
		"typescript": "5.4.5",
		"vitest": "^1.6.0",
		"wrangler": "^3.60.0"
	},
	"packageManager": "pnpm@9.2.0"
}
